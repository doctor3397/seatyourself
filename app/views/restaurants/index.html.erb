<% @restaurants.each do |restaurant| %>
<%= link_to restaurant_path(restaurant) %>

<%= image_tag restaurant.image %>

 <div>
   <h3>Name: <%= restaurant.name %></h3>
   <p>City: <%= restaurant.city %></p>
   <p>Name: <%= restaurant.name %></p>
   <p>Timeslot: <%= restaurant.timeslot %></p>
 </div>

<% end %>
